RESTART
phi1: READ FILE=COLVAR VALUES=phi1 IGNORE_TIME IGNORE_FORCES EVERY=1
phi2: READ FILE=COLVAR VALUES=phi2 IGNORE_TIME IGNORE_FORCES EVERY=1
psi1: READ FILE=COLVAR VALUES=psi1 IGNORE_TIME IGNORE_FORCES EVERY=1
psi2: READ FILE=COLVAR VALUES=psi2 IGNORE_TIME IGNORE_FORCES EVERY=1
omega1: READ FILE=COLVAR VALUES=omega1 IGNORE_TIME IGNORE_FORCES EVERY=1
omega2: READ FILE=COLVAR VALUES=omega2 IGNORE_TIME IGNORE_FORCES EVERY=1

PBMETAD ...
ARG=phi1,phi2,psi1,psi2,omega1,omega2
SIGMA=0.35,0.35,0.35,0.35,0.35,0.35
HEIGHT=0 #kJ/mol
PACE=500000
TEMP=300.0
BIASFACTOR=30
GRID_MIN=-pi,-pi,-pi,-pi,-pi,-pi
GRID_MAX=pi,pi,pi,pi,pi,pi
LABEL=pb
... PBMETAD
PRINT ARG=phi1,phi2,psi1,psi2,omega1,omega2,pb.bias STRIDE=1 FILE=colvar_reweight.dat
